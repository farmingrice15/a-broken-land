class Character:
  def __init__(self, name, attribute, role):
      self.name = name
      self.attribute = attribute
      self.role = role


player1 = Character("Ilyan", "Passive", "Explorer")
print(player1.name)
print(player1.attribute)
print(player1.role)

class Character:
  def __init__(self, name, attribute, role):
      self.name = name
      self.attribute = attribute
      self.role = role


player2 = Character("Kaela", "Passive", "Explorer")
print(player2.name)
print(player2.attribute)
print(player2.role)

